.money {
    color: #f9aa30 !important;
}

/* order  */

.order-attr {
    height: 200rpx;
    background-color: #fff;
    padding: 30rpx 35rpx;
    display: flex;
    flex-direction: column;
}

.order-all {
    height: 38.55555%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 10rpx;
    border-bottom: 1px solid #e5e5e5;
}

.all-title {
    font-family: PingFangSC-Medium;
    font-size: 16px;
    font-weight: bold;
    color: #353535;
    letter-spacing: 0;
    text-align: center;
}

.all-nav {
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #999;
    letter-spacing: -0.29px;
    display: flex;
    align-items: center;
}

.all-nav::after {
    content: "";
    width: 15rpx;
    height: 15rpx;
    border-top: 1rpx solid #ccc;
    border-right: 1rpx solid #ccc;
    transform: rotate(45deg);
}

.order-nav-arr {
    height: 61.4444444%;
    display: flex;
    margin-top: 20rpx;
    padding: 10rpx 5rpx;
}

.order-nav {
    width: 25%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: relative;
}

.order-nav-img {
    width: 50rpx;
    height: 50rpx;
}

.order-nav-point {
    width: 20rpx;
    height: 20rpx;
    position: absolute;
    right: 20rpx;
    top: 0rpx;
}

.order-nav text {
    font-size: 12px;
    margin-top: 10rpx;
}

/*  */

.plr {
    padding: 0 35rpx;
    background-color: #fff;
}

.flex-box {
    position: relative;
}

.flex-box image {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    left: 20rpx;
    top: 15rpx;
}
